<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 400">
  <!-- Background -->
  <rect width="800" height="400" fill="#ecfdf5" />
  
  <!-- Graduation Cap -->
  <g transform="translate(340, 150)">
    <rect x="-80" y="20" width="160" height="10" fill="#047857" />
    <rect x="-80" y="20" width="160" height="50" fill="#10b981" />
    <polygon points="0,-20 100,20 0,60 -100,20" fill="#059669" />
    <circle cx="0" cy="-20" r="20" fill="#10b981" />
    <rect x="-10" y="-20" width="20" height="120" fill="#10b981" />
    <rect x="-30" y="100" width="60" height="20" fill="#047857" />
  </g>
  
  <!-- Books -->
  <g transform="translate(150, 220)">
    <rect x="0" y="0" width="70" height="100" fill="#3b82f6" rx="5" />
    <rect x="10" y="20" width="50" height="10" fill="#ffffff" />
    <rect x="10" y="40" width="50" height="10" fill="#ffffff" />
    <rect x="10" y="60" width="30" height="10" fill="#ffffff" />
  </g>
  <g transform="translate(140, 200)">
    <rect x="0" y="0" width="70" height="100" fill="#8b5cf6" rx="5" />
    <rect x="10" y="20" width="50" height="10" fill="#ffffff" />
    <rect x="10" y="40" width="50" height="10" fill="#ffffff" />
    <rect x="10" y="60" width="30" height="10" fill="#ffffff" />
  </g>
  
  <!-- Pencil -->
  <g transform="translate(580, 180) rotate(45)">
    <rect x="0" y="0" width="150" height="20" fill="#fbbf24" />
    <polygon points="0,0 0,20 -30,10" fill="#4b5563" />
    <rect x="150" y="0" width="20" height="20" fill="#f87171" />
  </g>
</svg>