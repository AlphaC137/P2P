<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 400">
  <!-- Sky Background -->
  <rect width="800" height="400" fill="#f0f9ff" />
  
  <!-- Sun -->
  <circle cx="680" cy="80" r="60" fill="#fbbf24" />
  <g stroke="#f97316" stroke-width="4">
    <line x1="680" y1="0" x2="680" y2="20" />
    <line x1="680" y1="140" x2="680" y2="160" />
    <line x1="600" y1="80" x2="620" y2="80" />
    <line x1="740" y1="80" x2="760" y2="80" />
    <line x1="624" y1="24" x2="638" y2="38" />
    <line x1="722" y1="122" x2="736" y2="136" />
    <line x1="624" y1="136" x2="638" y2="122" />
    <line x1="722" y1="38" x2="736" y2="24" />
  </g>
  
  <!-- House -->
  <polygon points="150,250 400,150 650,250 650,380 150,380" fill="#e2e8f0" />
  <polygon points="400,150 650,250 150,250" fill="#94a3b8" />
  <rect x="350" y="300" width="100" height="80" fill="#475569" />
  
  <!-- Solar Panels -->
  <g transform="translate(200, 220)">
    <rect x="0" y="0" width="320" height="100" fill="#1e293b" />
    <line x1="0" y1="25" x2="320" y2="25" stroke="#e2e8f0" stroke-width="2" />
    <line x1="0" y1="50" x2="320" y2="50" stroke="#e2e8f0" stroke-width="2" />
    <line x1="0" y1="75" x2="320" y2="75" stroke="#e2e8f0" stroke-width="2" />
    <line x1="80" y1="0" x2="80" y2="100" stroke="#e2e8f0" stroke-width="2" />
    <line x1="160" y1="0" x2="160" y2="100" stroke="#e2e8f0" stroke-width="2" />
    <line x1="240" y1="0" x2="240" y2="100" stroke="#e2e8f0" stroke-width="2" />
  </g>
  
  <!-- Energy Lines -->
  <g stroke="#fbbf24" stroke-width="3" stroke-dasharray="8,8">
    <line x1="680" y1="80" x2="520" y2="230" />
    <line x1="680" y1="80" x2="450" y2="230" />
    <line x1="680" y1="80" x2="380" y2="230" />
    <line x1="680" y1="80" x2="310" y2="230" />
    <line x1="680" y1="80" x2="240" y2="230" />
  </g>
</svg>